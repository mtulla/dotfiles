# dd-curl to test APIs
dd-curl() {
  local url host
  local -a auth=(dd-auth)
  for url in "$@"; do [[ $url == http*://* ]] && break; url=""; done
  if [[ -n $url ]]; then
    host=${url#*://}; host=${host%%[/:]*}; host=${host#api.}
    [[ $host == datadoghq.eu ]] && host=app.datadoghq.eu
    [[ $host == datad0g.com ]] && host=dd.datad0g.com
    [[ $host != datadoghq.com ]] && auth+=(--domain "$host")
  fi

  "${auth[@]}" -- bash -c '
    curl -sS \
      -H "Accept: application/json" \
      -H "DD-API-KEY: $DD_API_KEY" \
      -H "DD-APPLICATION-KEY: $DD_APP_KEY" \
      "$@"
  ' _ "$@"
}

alias dd-api-key="dd-auth -o | grep DD_API_KEY | cut -d= -f2 | pbcopy && echo 'DD_API_KEY copied to clipboard'"
alias dd-app-key="dd-auth -o | grep DD_APP_KEY | cut -d= -f2 | pbcopy && echo 'DD_APP_KEY copied to clipboard'"
